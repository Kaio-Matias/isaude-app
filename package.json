{
  "name": "isaude-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:cli": "tsc -p tsconfig.cli.json",
    "typeorm": "node -r dotenv/config -r tsconfig-paths/register ./dist-cli/lib/config/database.js",
    "migration:run": "npm run build:cli && typeorm-ts-node-commonjs -d dist-cli/lib/config/database.js migration:run",
    "migration:generate": "npm run build:cli && typeorm-ts-node-commonjs -d src/lib/config/database.ts migration:generate"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@supabase/supabase-js": "^2.52.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.17.1",
    "firebase-admin": "^12.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.3",
    "nodemailer": "^6.9.14",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
